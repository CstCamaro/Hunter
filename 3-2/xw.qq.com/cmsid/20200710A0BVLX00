:none;white-space:nowrap;}.nav.jsx-3712844012 ol.jsx-3712844012 li.jsx-3712844012 a.placeholder.jsx-3712844012{color:#bbb;}.nav.jsx-3712844012 ol.jsx-3712844012 li.jsx-3712844012 a.reddot.jsx-3712844012::after{content:'';display:block;position:absolute;top:4px;right:0px;background:#ed5151;width:6px;height:6px;border-radius:50%;}@media screen and (max-width:350px){.nav.jsx-3712844012 ol.jsx-3712844012 li.jsx-3712844012 a.text-length-4.jsx-3712844012{font-size:12px;}}.nav.skin-2019ny.jsx-3712844012{background-color:#FFF4F0;}</style><style id="__jsx-287773377">.footer.jsx-287773377{background:#F7F8FA;color:#A8ABB0;font-size:12px;padding:23px 0;text-align:center;position:relative;}.footer.jsx-287773377 .links.jsx-287773377{margin-bottom:15px;}.footer.jsx-287773377 .links.jsx-287773377 a.jsx-287773377{color:#7D8189;-webkit-text-decoration:none;text-decoration:none;display:inline-block;margin-right:10px;}.footer.jsx-287773377 .links.jsx-287773377 a.jsx-287773377+a.jsx-287773377{padding-left:10px;border-left:1px solid #E0E0E0;}.copyright.jsx-287773377{font-size:12px;}</style><style id="__jsx-2472896812">.top-red.jsx-2472896812{display:block;position:relative;height:58px;}.audio-wrap.jsx-2472896812{margin:0 0.18rem 0.12rem;height:0.4rem;}audio.jsx-2472896812{width:100%;height:100%;}.follow_bottom.jsx-2472896812{position:absolute;top:16px;right:0;}.isom.jsx-2472896812{margin:15px 0 20px;font-size:14px;color:#909aa4;line-height:20px;position:relative;}.primary.jsx-2472896812{padding:0 0.18rem;position:relative;}#article_body.jsx-2472896812{margin:0 0.18rem;overflow:hidden;position:relative;max-height:1010px;}#article_body.packed.jsx-2472896812{max-height:1000px;}#article_body.unpack.jsx-2472896812{max-height:none;}.mask.jsx-2472896812{background:url(//mat1.gtimg.com/www/mobi/2017/image/linearlayer.png) repeat-x;background:linear-gradient( -180deg, rgba(255,255,255,0) 0%, rgba(255,255,255,0.6) 49%, #ffffff 85% );position:absolute;width:100%;bottom:0;left:0;height:70px;z-index:5;border:none;}.collapseWrapper.jsx-2472896812{margin-bottom:0.24rem;position:relative;top:4px;background:#fff;z-index:5;}.comment_download.jsx-2472896812{background:#ededed;display:block;-webkit-text-decoration:none;text-decoration:none;border-radius:6px;font-size:14px;height:40px;position:relative;text-align:center;line-height:40px;color:#56bb56;margin:0 12px 20px;}.comment_download.jsx-2472896812 img.jsx-2472896812{display:inline-block;vertical-align:middle;width:19px;margin-top:-2px;}.landad.jsx-2472896812{position:relative;z-index:4;}.icon-content-more.jsx-2472896812{display:block;height:0.32rem;width:0.32rem;margin:0px auto;}.content-more.jsx-2472896812{height:0.46rem;background:rgba(69,116,255,1);border-radius:0.06rem;margin:0.16rem 0.18rem;font-size:0.16rem;font-family:PingFangSC-Regular,PingFang SC;color:rgba(255,255,255,1);line-height:0.46rem;text-align:center;}.channel-entry.jsx-2472896812 a.jsx-2472896812{display:block;margin-bottom:0.16rem;text-align:center;font-size:0.15rem;font-weight:500;color:#537bfe;line-height:0.16rem;-webkit-text-decoration:none;text-decoration:none;}.channel-entry.jsx-2472896812 a.jsx-2472896812::after{content:'';background:url(//mat1.gtimg.com/pingjs/ext2020/xw-article-icons/icon_article_more.svg) center no-repeat;background-size:contain;display:inline-block;width:0.16rem;height:0.16rem;vertical-align:text-bottom;}</style><style id="__jsx-3538576898">@-webkit-keyframes showTips-jsx-3538576898{0%{-webkit-transform:translateY(-100%);-ms-transform:translateY(-100%);transform:translateY(-100%);}10%{-webkit-transform:translateY(0);-ms-transform:translateY(0);transform:translateY(0);}90%{-webkit-transform:translateY(0);-ms-transform:translateY(0);transform:translateY(0);}100%{-webkit-transform:translateY(-100%);-ms-transform:translateY(-100%);transform:translateY(-100%);}}@keyframes showTips-jsx-3538576898{0%{-webkit-transform:translateY(-100%);-ms-transform:translateY(-100%);transform:translateY(-100%);}10%{-webkit-transform:translateY(0);-ms-transform:translateY(0);transform:translateY(0);}90%{-webkit-transform:translateY(0);-ms-transform:translateY(0);transform:translateY(0);}100%{-webkit-transform:translateY(-100%);-ms-transform:translateY(-100%);transform:translateY(-100%);}}.tips.jsx-3538576898{display:none;position:fixed;top:0;left:0;z-index:8;overflow:hidden;width:100%;min-height:0.3rem;line-height:0.2rem;padding:0.05rem;box-sizing:border-box;text-align:center;word-break:break-all;color:#fff;font-size:0.14rem;background-color:rgba(68,138,255,.9);-webkit-transform:translateY(-100%);-ms-transform:translateY(-100%);transform:translateY(-100%);will-change:transform;}.tips.show.jsx-3538576898{display:block;-webkit-animation-name:showTips-jsx-3538576898;animation-name:showTips-jsx-3538576898;-webkit-animation-duration:2800ms;animation-duration:2800ms;-webkit-animation-iteration-count:1;animation-iteration-count:1;}</style></head><body><div id="__next"><div style="position:relative" class="__xw_app"><div class="jsx-3967532495 root"><div class="jsx-3967532495 inner "><div id="mod_player" class="jsx-3967532495 tvp-wrap"></div></div></div><div id="__xw_float_view_root"></div><div id="__xw_next_view_root"><div style="max-width:768px;margin:0px auto" class="jsx-2472896812"><div style="visibility:hidden" class="jsx-2472896812"><div class="headroom-wrapper"><div style="position:relative;top:0;left:0;right:0;z-index:11;-webkit-transform:translate3D(0, 0, 0);-ms-transform:translate3D(0, 0, 0);transform:translate3D(0, 0, 0);max-width:768px;margin:0 auto" class="headroom headroom--unfixed"><div class="jsx-1225297045 ch-nav"><div class="jsx-1355058077 main average"><a data-boss="home&amp;modular=head" href="/" target="_self" class="jsx-1355058077 gohome ">首页</a><div class="jsx-1355058077 title"><a style="-webkit-tap-highlight-color:transparent" class="jsx-3067894286 ">新闻</a></div><a href="/m/s/sort" data-boss="qzdh&amp;modular=head" class="jsx-1355058077 more sort"><img src="data:image/svg+xml;base64," alt="更多频道" class="jsx-1355058077"/></a></div></div></div></div></div><div class="jsx-2472896812 main-wrapper "><div class="jsx-2472896812 primary"><h1 class="jsx-717484942 title">他17岁在自行车上做起创业梦，如今他让一种米的卖价提升10倍</h1><div class="jsx-717484942 meta"><a href="/m/author?id=17111844" data-boss="author&amp;modular=author" class="jsx-717484942 author omauthor"><i class="jsx-717484942"></i><span class="jsx-717484942">致富君</span></a><div id="subtime" class="jsx-717484942 pubtime">07-10 11:51</div></div></div><div id="article_body" class="jsx-2472896812 "><div class="jsx-428399900"><div class="jsx-428399900 text">有人说人生在世，不能白来一趟，总要留下点什么才行。可一个人到底能做些什么？能留下什么呢？小的时候，很多人就立志要做科学家、航天员、医生、律师......等到长大后，回望自己儿时的梦想，大多数人都会说一句：“小时候不懂事，瞎说的”。那么到底是当初的梦想太过遥远，还是没能真的坚持下去呢？</div><div class="jsx-428399900 text">今天我们要讲的这位，来自四川省自贡市荣县，他叫吴剑。1992年，吴剑考上了自贡市农业学校，就读中专，学的是农学专业。他在这里遇上了对自己的人生影响很大的一个人。</div><div data-boss="pic&amp;modular=content" class="jsx-428399900 pic"><div style="max-width:641px" class="jsx-428399900 responsive-image"><div style="padding-top:56.32%" class="jsx-428399900 sizer"></div><div data-boss-expo="pic&amp;modular=content" data-boss-once="true" class="jsx-428399900 image-wrap"></div></div></div><div class="jsx-428399900 text"><STRONG>自行车上的雄心壮志</STRONG></div><div class="jsx-428399900 text">吴剑在读中专时的班主任叫李邦荣，刚刚参加工作的李邦荣因为和吴剑交谈时的一句话，被深深的打动，也成为吴剑创业的第一个支持者。 吴剑和李邦荣聊天时，说学习农学专业，就是想要自己做点事情，为荣县农业做点事。 面对一个刚上中专的孩子的话语，李邦荣没有一笑置之，反而重视了起来。1992年时，一辆自行车需要两三百块钱，李邦荣省吃俭用三个月攒钱买的一辆自行车，自己还没骑几次，就主动借给了吴剑，还鼓励他出去闯一闯。</div><div data-boss="pic&amp;modular=content" class="jsx-428399900 pic"><div style="max-width:641px" class="jsx-428399900 responsive-image"><div style="padding-top:56.32%" class="jsx-428399900 sizer"></div><div data-boss-expo="pic&amp;modular=content" data-boss-once="true" class="jsx-428399900 image-wrap"></div></div></div><div class="jsx-428399900 text">就因为吴剑的一句话，李邦荣把自己的宝贝自行车借给了吴剑，让他骑着去跑业务，吴剑开始做起了卖花岗岩的生意，每天骑着小自行车走街串巷，到工地上揽生意，风雨无阻。 奔波了半年，终于迎来了自己的第一单生意。 万事开头难，有了一单生意，吴剑入了门。1996年毕业时候，靠卖花岗岩吴剑已经攒了六七十万元。小自行车也换成了大摩托车。可毕业以后，吴剑却放弃了这门生意，而是跑到一个偏僻的小山村，开始试种各种果树、还养鸡养鸭养兔子，为的就是积累实践经验。吴剑在履行着当初和老师的承诺。 2006年，机会终于来了。四川盛产白酒，酒厂都需要大量的高粱来酿酒，可那时候当地高粱种植量少还不成规模，吴剑就把大家组织起来成立合作社种高粱。到2011年，全县12个乡都有人跟着吴剑的合作社种高粱。可吴剑觉得这还远远不够，也就是在这一年，他又发现一个更大的商机。</div><div data-boss="pic&amp;modular=content" class="jsx-428399900 pic"><div style="max-width:641px" class="jsx-428399900 responsive-image"><div style="padding-top:56.32%" class="jsx-428399900 sizer"></div><div data-boss-expo="pic&amp;modular=content" data-boss-once="true" class="jsx-428399900 image-wrap"></div></div></div><div class="jsx-428399900 text">当时荣县有50万亩稻田，当地的大米2块2一斤都卖不出去，东北大米4块多还抢着买，如果能在稻米上做出些文章，肯定能带着乡亲们挣钱。 经过考察，吴剑发现当地的稻米有个很大的弊端，那就是品种太老而且种类很杂，如果能引进优质高产的新品种，肯定会有改观。 吴剑花了三年时间，从十几种品种中，挑选出了一个挑选出一个适合在当地种的水稻品种，而且很快就接了一笔上万吨的水稻订单。他觉得带着大家一起赚钱的时刻到了。 为了鼓励大家种植这种水稻，吴剑给出的收购价，比当地平均水稻收购价每斤高出3毛钱，而且包收购。都是种水稻，大家一看有钱挣，很多老百姓主动上门找吴剑签订合同。短短4个月，要给吴剑种水稻的人就猛增到上万人。为了方便管理，吴剑在每个片区都挑了一个人做区域经纪人，光经纪人都有五十多个。因为时间紧凑，很多经纪人都是临时上马，没收过水稻。</div><div data-boss="pic&amp;modular=content" class="jsx-428399900 pic"><div style="max-width:641px" class="jsx-428399900 responsive-image"><div style="padding-top:56.32%" class="jsx-428399900 sizer"></div><div data-boss-expo="pic&amp;modular=content" data-boss-once="true" class="jsx-428399900 image-wrap"></div></div></div><div class="jsx-428399900 text">一张张合同摆在吴剑面前，对于吴剑来说，压力也更大了看着各处的水稻都长势很好，他才稍稍安心了一点。可没想到，2014年秋天，还是出事了。</div><div class="jsx-428399900 text"><STRONG>赔钱为梦想</STRONG></div><div class="jsx-428399900 text">2014年9月，吴剑正式开仓收粮。可收粮一个星期后，收粮的员工觉得不对劲，当初发出去的种子，能收回来多少粮食，吴剑的团队都是经过计算的，这才一个星期，收上来的稻谷每亩都超过当初的预期。这是咋回事？是当初计算失误？还是丰收了？ 吴剑觉得事情不是这么简单，赶忙跑到粮仓检查粮食，一下子就傻了眼。收上来的稻谷，起码有三分之一不是自己规定的品种，混进来了许多杂粮。 由于很多区域经纪人都没有收粮经验，导致很多其他品种的粮食混了进去，而且经纪人在收粮时，都是提前垫付资金，如果吴剑不收粮，损失都要由他们自己来承担。 吴剑的心里更纠结了，自己如果就此不收粮了，确实能够及时止损，但是当初给农户的承诺该咋办？如果这次不收了，估计也就没人再和他合作了，想要带领乡亲赚钱的想法也就落空了。</div><div data-boss="pic&amp;modular=content" class="jsx-428399900 pic"><div style="max-width:641px" class="jsx-428399900 responsive-image"><div style="padding-top:56.32%" class="jsx-428399900 sizer"></div><div data-boss-expo="pic&amp;modular=content" data-boss-once="true" class="jsx-428399900 image-wrap"></div></div></div><div class="jsx-428399900 text">吴剑知道，因为规模的迅速扩大，自己考虑不全面，团队不够专业，才出现了这样的局面。自己也有很大责任，他提出已经入了粮库的1000吨稻谷，损失由他承担。但是从这天开始，以后谁拉来的稻谷再出问题，就由经纪人自己承担损失。 1000吨入仓的稻谷，吴剑赔了70多万元。这次赔钱给吴剑敲响了警钟，他开始培养专业的团队，防止类似的事情再次发生。 种水稻，按照一家10亩地算，年收入在1.5万元左右，确实让乡亲们挣了钱，可想要让农户们安心，这还远远不够。从小在农村长大的吴剑，开始琢磨在稻田里套养点什么，他最先想到了鱼，可农户养鱼的模式比较粗放，套养一亩最多产出500块钱，纯利润更没多少。 吴剑觉得套养模式没错，重要的是养什么，又开始研究青蛙，养青蛙确实赚钱，但养蛙的基础建设又太贵，一亩地起码投入5万元左右，一般农户根本拿不出这么多钱。养青蛙的想法又被自己否定了。</div><div data-boss="pic&amp;modular=content" class="jsx-428399900 pic"><div style="max-width:641px" class="jsx-428399900 responsive-image"><div style="padding-top:56.32%" class="jsx-428399900 sizer"></div><div data-boss-expo="pic&amp;modular=content" data-boss-once="true" class="jsx-428399900 image-wrap"></div></div></div><div class="jsx-428399900 text">来来回回折腾了两年，吴剑不断向稻田里投放各种东西，可都没能找到合适的。到底能养点啥，又方便，又挣钱呢？ 2017年的一天，吴剑来到成都一家饭店吃饭，无意中听说了个新鲜玩意，吴剑觉得，这就是他一直在苦苦寻找的项目。</div><div class="jsx-428399900 text"><STRONG>小龙虾带来新希望</STRONG></div><div class="jsx-428399900 text">小龙虾一直是餐饮市场上的爆款，每年都会有上百万吨的小龙虾走上餐桌。而澳洲小龙虾的卖价是普通小龙虾的2倍。由于养殖技术要求高，澳洲小龙虾当时在大陆养殖的还很少，可在台湾，这种澳洲小龙虾的养殖技术已经很成熟了</div><div data-boss="pic&amp;modular=content" class="jsx-428399900 pic"><div style="max-width:641px" class="jsx-428399900 responsive-image"><div style="padding-top:56.32%" class="jsx-428399900 sizer"></div><div data-boss-expo="pic&amp;modular=content" data-boss-once="true" class="jsx-428399900 image-wrap"></div></div></div><div class="jsx-428399900 text">吴剑赶忙订了机票，飞到台湾找到了一家专业养殖这种龙虾的公司了解情况。澳洲小龙虾相比普通小龙虾，螯很小，而且不会打洞，更适合在水稻田里养。吴剑花70万元，和台湾那家公司签了一年的技术指导合同。2018年初，对方派了一个3人的技术团队来到四川荣县。</div><div class="jsx-428399900 text">应对方要求，吴剑公司投资3百万元建起占地3000平方米的繁育池。繁育是养殖的关键环节，不仅投入大，而且技术难度也很大，吴剑团队的人想要学技术，却被拒之门外，技术人员告诉他们，核心技术，不外传。 2018年5月，澳洲小龙虾真的开始孵化出小虾苗，看着他们，吴剑好像看到了希望。可好景不长。因为每年收稻谷和新建澳洲小龙虾繁育池，都用掉了大量资金，每个月维持运转都需要20万元左右，当对方技术团队提出要接一条单独的饮水管线到龙虾繁育池时，吴剑的公司已经拿不出那么多钱了。</div><div data-boss="pic&amp;modular=content" class="jsx-428399900 pic"><div style="max-width:641px" class="jsx-428399900 responsive-image"><div style="padding-top:56.32%" class="jsx-428399900 sizer"></div><div data-boss-expo="pic&amp;modular=content" data-boss-once="true" class="jsx-428399900 image-wrap"></div></div></div><div class="jsx-428399900 text">一分钱难倒英雄汉，更何况是一大笔钱。没办法吴剑只能用水库的水养虾，技术人员反复告诫他，这样肯定会出问题，可吴剑也无计可施。很快一年技术指导期过，技术团队看吴剑一直没办法完善硬件设施，决定不再和他续约。 2018年底，技术团队一走，果然出事了。 种虾开始出现大规模死亡，如果全死光，那就是要面临上百万元的损失。可天无绝人之路，正巧这时候市政规划的一条饮水管线铺设完成，正好经过他的繁育基地，才解了吴剑的燃眉之急。可对方技术团队已经撤走，吴剑又还没完全掌握养殖技术，种虾活下来的也不多，吴剑该咋办？ 他先把一半的繁育池停止运作，其他存活下来的种虾集中在另外一半繁育池里，这样即节省成本，又便于他观察虾的生长情况。就这么每天从早到晚围着这些虾转悠，吴剑慢慢地发现了问题。 四川的冬天气温，相比台湾更低，吴剑给繁育池加建了简单的供暖设备，死虾的情况果然有所改观，到了2019年4月，他也可以自己繁育出来小虾苗了。</div><div data-boss="pic&amp;modular=content" class="jsx-428399900 pic"><div style="max-width:641px" class="jsx-428399900 responsive-image"><div style="padding-top:56.32%" class="jsx-428399900 sizer"></div><div data-boss-expo="pic&amp;modular=content" data-boss-once="true" class="jsx-428399900 image-wrap"></div></div></div><div class="jsx-428399900 text">当吴剑把繁育出来的虾苗从繁育池放到稻田里去养的时候，死虾的情况又出现了...... 吴剑每天就在稻田边上转悠，可怎么也想不通，一天，有客户上门，吴剑捞起几斤虾带到食堂，准备第二天给客户品尝，第二天一早，他却发现，昨天带回来的虾几乎全死了。 过了几天，吴剑又捞了几斤虾，正好看见食堂的水池里有条鱼，就顺手把虾也放了进去。可没想到第二天这些虾一只都没死。 这两次唯一的区别就是那条鱼，平时食堂买来鱼，都会放在水池里，因为怕鱼死掉，都打开增氧机通点氧气。 “小龙虾需要大量的氧气”这个念头在吴剑心中升起，他赶快把稻田里都引进了增氧设备，从那以后再也没有出现过死虾。 一亩稻田可以养300斤左右澳洲小龙虾，2019年，吴剑以每斤85钱的价格，把它们卖到成都的十几家饭店，几乎每天都供不应求。 掌握了繁育和养殖技术后，吴剑开始培育经过本土化的澳洲小龙虾，确保向农户推广时，养殖的风险性更低。这么大费周折的寻找让农户增收的项目，为的就是让更多人愿意跟着他种水稻，吴剑有个筹备了5年的计划，要实施。 2014年，有人看上了吴剑的魄力和灵活的思路，给他投了700万元。吴剑看中了一个特别的产品，同样的水稻，却可以把卖价翻十倍。</div><div data-boss="pic&amp;modular=content" class="jsx-428399900 pic"><div style="max-width:641px" class="jsx-428399900 responsive-image"><div style="padding-top:56.32%" class="jsx-428399900 sizer"></div><div data-boss-expo="pic&amp;modular=content" data-boss-once="true" class="jsx-428399900 image-wrap"></div></div></div><div class="jsx-428399900 text">水稻采收后，先经过一次脱壳，得到的是糙米，再经过一次脱皮，就成了精白米。随着大家对健康饮食的追求，糙米这样的粗粮，开始得到更多关注。 可消费者对于糙米的反应却是：难吃、口感很涩、难以下咽等词汇。吴剑不是这方面的专家，直到2014年自贡市引进浙江大学创新研究中心，他知道自己的机会来了。</div><div class="jsx-428399900 text"><STRONG>难题是用来攻克的</STRONG></div><div class="jsx-428399900 text">当时在日本有一种新技术，让糙米二次萌芽，这样可以让糙米的营养价值变得更高，味道也更好。浙江大学研究团队，也同样掌握了让糙米二次萌芽的技术。可这种技术带来的问题就是，很难保存。</div><div data-boss="pic&amp;modular=content" class="jsx-428399900 pic"><div style="max-width:641px" class="jsx-428399900 responsive-image"><div style="padding-top:56.32%" class="jsx-428399900 sizer"></div><div data-boss-expo="pic&amp;modular=content" data-boss-once="true" class="jsx-428399900 image-wrap"></div></div></div><div class="jsx-428399900 text">浙江大学研究团队，通过几年的研究才找到了一个可能行得通的办法。当听到有可能攻破这个难题时，吴剑高兴的差点跳起来。可没想到，专家的下一句话，就给他泼了一盆冷水。研究人员告诉吴剑，预糊化技术的时间非常重要，而且以现在的了解，并不知道这个时间区间，只能一次次尝试。成千上万次的实验，才有可能找到合适的时间点。 吴剑和研究人员，花费了五年时间，做了五千多次试验，从间隔5分钟直到尝试到间隔5秒钟，终于找到了适合做萌芽糙米的水稻品种，还测算出这些水稻品种所需要的，不同的预糊化时间点。</div><div data-boss="pic&amp;modular=content" class="jsx-428399900 pic"><div style="max-width:641px" class="jsx-428399900 responsive-image"><div style="padding-top:56.32%" class="jsx-428399900 sizer"></div><div data-boss-expo="pic&amp;modular=content" data-boss-once="true" class="jsx-428399900 image-wrap"></div></div></div><div class="jsx-428399900 text">2019年，吴剑正式建起了糙米加工厂，里面的每一台设备，都是他们自己设计和改造的，可以支持每天生产50吨萌芽糙米。截止记者采访时，已经有3家大型公司和吴剑签署了糙米购买合同，每斤订购价36元。 现在，吴剑牵头成立的种养合作社，共有社员3150户，水稻种植面积超3万亩。</div><div class="jsx-428399900 text">稻田里套养澳洲小龙虾，生产的稻谷都加工成萌芽糙米，每亩稻田的纯收入比之前提升了五六倍，吴剑已经在当地把这个模式逐步推广开来。</div></div><div data-boss-expo="end&amp;modular=content" data-boss-once="true" class="jsx-2472896812"></div><div class="jsx-2472896812 isom">免责声明：本文来自腾讯新闻客户端自媒体，不代表腾讯新闻、腾讯网的观点和立场。</div></div><div style="height:0.01rem" class="jsx-1893092673 hairline-top"></div><div class="jsx-671811996 gap"></div><div data-boss-expo="&amp;modular=map" class="jsx-2472896812"><div style="background-color:#fff" class="jsx-3712844012 nav skin-default"><ol class="jsx-3712844012"><li class="jsx-3712844012"><a href="/m/s/sort" data-boss="map&amp;map=more&amp;modular=bottom" class="jsx-3712844012">更多</a></li></ol></div></div><div style="top:0" class="jsx-287773377 footer hasnav"><div class="jsx-287773377 links"><a href="https://support.qq.com/products/14842" data-boss="feedback&amp;modular=bottom" class="jsx-287773377 feedback">反馈</a><a href="http://report.12377.cn:13225/toreportinputNormal_anis.do" data-boss="rcenter&amp;modular=bottom" class="jsx-287773377">举报中心</a><a href="https://privacy.qq.com/yszc-m.htm" data-boss="privacy&amp;modular=bottom" class="jsx-287773377">隐私政策</a><a href="https://new.qq.com/omn/20200710/20200710A0BVLX00.html?pc" data-boss="pc&amp;modular=bottom" class="jsx-287773377 pcs">电脑版</a></div><div class="jsx-287773377 copyright">Copyright © 1998 - <!-- -->2020<!-- --> Tencent. All Rights Reserved</div></div></div><div data-boss="message" class="jsx-2472896812"></div></div></div><div class="jsx-3538576898 tips "><span class="jsx-3538576898"></span></div></div><script src="//mat1.gtimg.com/www/https/pingjs20161020.js"></script></div><script id="__NEXT_DATA__" type="application/json">{"dataManager":"[]","props":{"pageProps":{"pageConfig":{"key":"article","disableAutoPush":true,"includeManifest":true,"registerServiceWorker":true,"includeGSV":true,"remLayout":{"enabled":true,"range":[300,414]},"bossConfig":{"options":{"pagetype":"article","channel":"","page":"ptdc","pre_id":"20200710A0BVLX00","category_ori":""}},"pgvConfig":{"options":{"pgv_site":"cmsid","useUnofficialJS":true}}},"data":{"data":{"channel":["biglife","ywexpand","sannong"],"content":{"abstract":"有人说人生在世，不能白来一趟，总要留下点什么才行。可一个人到底能做些什么？能留下什么呢？小的时候，很多人就立志要做科学家、航天员、医生、律师......等到长大后，回望自己儿时的梦想，大多数人都会说……","abstract_count":300,"abstract_pad":"有人说人生在世，不能白来一趟，总要留下点什么才行。可一个人到底能做些什么？能留下什么呢？小的时候，很多人就立志要做科学家、航天员、医生、律师......等到长大后，回望自己儿时的梦想，大多数人都会说一句：“小时候不懂事，瞎说的”。那么到底是当初的梦想太过遥远，还是没能真的坚持下去呢？　今天我们要讲的这位，来自四川省自贡市荣县，他叫吴剑。1992年，吴剑考上了自贡市农业学校，就读中专，学的是农学专业。他在这里遇上了对自己的人生影响很大的一个人。　自行车上的雄心壮志　吴剑在读中专时的班主任叫李邦荣，刚刚参加工作的李邦荣因为和吴剑交谈时的一句话，被深深的打动，也成为吴剑创业的第一个支持者。 吴剑和李邦荣聊天时","alt_time":"2020-07-10 1
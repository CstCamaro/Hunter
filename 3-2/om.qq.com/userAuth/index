<link rel="stylesheet" href="//om.gtimg.cn/om/om_2.0/libs/chosen/chosen.min.css">
<script type="text/javascript" src="//om.gtimg.cn/om/om_2.0/baselibs/baselibs.9a65ca.js"></script>
<script type="text/javascript" src="//om.gtimg.cn/om/om_2.0/libs/layer/layer.min.js"></script>
<script type="text/javascript" src="//om.gtimg.cn/om/om_2.0/js/babel/polyfill.min.js"></script>

<script>
$(function(){
    var dd, funel = funel || 150;
    var $menu = $(".header-login .dropdown-menu"),
        $nick = $(".header-login .user .name");

    var $sidemenu = $('.side .menu');

    $sidemenu.on('click', '.tooltip .pop-close', function(){
        $(this).closest('.menu-item-tips-w').find('.menu-item-tips-w').removeClass('menu-item-tips-w');
        $(this).closest('.tooltip').remove();
    })

    function showMenu() {
        clearTimeout(dd);
        $menu.show();
    }

    function hideMenu() {
        dd = setTimeout(function() {
            $menu.hide();
        }, funel);
    }
    $menu.on("mouseenter", function() {
        showMenu();
        return false;
    }).on("mouseleave", function() {
        hideMenu();
        return false;
    });
    $nick.on("mouseenter", function() {
        showMenu();
        return false;
    }).on("mouseleave", function() {
        hideMenu();
        return false;
    });

    $("#userNick a.link-shenfen").on("mouseenter", function(e) {
        var title = $(this).attr("title");
        if (title) {
            $(this).removeAttr("title");
            $(this).attr("data-title", title);
        } else {
            title = $(this).attr("data-title");
        }

        if (title) {
            var _this = this;
            layer.tips(title, $(_this), {
                time: 10000,
                tips: 1,
                success: function($container, index) {
                    var width = $container.width();
                    var left = parseInt($container.css("left"));
                    $container.css("left", left - width / 2 + 16);
                }
            });
        }
    }).on("mouseleave", function(e) {
        layer.closeAll();
    });

    $(".quick-dropdown-menu > li").eq(0).hover(function(){
        $(this).parents().find('.quick-dropdown-img').show();
    },function(){
        $(this).parents().find('.quick-dropdown-img').hide();
    });
    
    // 互动消息小红点
    (function($) {
        try {
            var cookie = {
                get: function(name, raw) {
                    var arr = document.cookie.match(new RegExp("(^| )" + name + "=([^;]*)(;|$)"));
                    if (arr != null) {
                        return raw ? arr[2] : unescape(arr[2]);
                    }
                    return null;
                }
            };

            if($(".menu li.interactiveMessage").length){
                if ("/interactive/message" == location.pathname) {
                    return null;
                }   
             
                var url = "/hd/" + g_userInfo.mediaId + "/noread";
                var params = {
                    omtoken: cookie.get("omtoken"),
                    msgtype: 1,
                    _: parseInt(Math.random() * 1E9)
                };
                $.ajax({
                    url: url,
                    data: params,
                    dataType: "json",
                    type: "get"
                }).then(function(resp) {
                    var $menu = $(".interactiveComment").find("a").first()
                    if (resp.data.num > 0) {
                        $menu.html('评论管理<span class="wake-mark">&#9679;</span>');
                    } else {
                        $menu.html('评论管理');
                    }
                });    
            }
        } catch (e) {}
        //手机号或者邮箱登录的用户，引导设置qq绑定或者微信绑定，或者重新登录。
        if ((localStorage.getItem('needBindQQ')||cookie.get("om_dia_tip")==1)&&(g_userInfo.logintype==1||g_userInfo.logintype==2)) {
            var isShowBindQQLayer=false;
            var bindQQLayerIndex;
            var showBindQQLayer=function(){
                if(isShowBindQQLayer){
                    return ;
                }
                isShowBindQQLayer=true;
                bindQQLayerIndex=layer.open({
                    type: 1,
                    area: ['670px', '380px'],
                    content: '<div class="pop" style="top: 10%;" id="bind-qq-pop"><div class="pop-heading text-center bor-bottom"><h4>温馨提示</h4></div><div class="pop-body"><div class="ycsm-form"><div class="intro-box" style="text-indent:2em;"><p>尊敬的企鹅号创作者：为保障您的帐号安全，企鹅号平台正在进行登录方式优化，未来平台将仅保留QQ和微信登录方式，为防止影响您的正常登录运营，若您未绑定过QQ或微信的登录方式，请立即绑定QQ或微信的登录设置。若您已有可用QQ或微信登录方式，请使用QQ或微信重新登录。感谢您的理解与支持。</p></div></div></div></div>',
                    btn: ['立即绑定','重新登录'],
                    closeBtn: 0,
                    title: false,
                    yes: function() {
                        if(location.href.indexOf('/user/accountSiginSettings')==-1){
                            location.href = '/user/accountSiginSettings';
                        }else{
                            //console.log(bindQQLayerIndex);
                            layer.close(bindQQLayerIndex);
                            isShowBindQQLayer=false;
                        }
                    },
                    btn2:function(){
                        location.href='/userAuth/index';
                    }
                })
                $('#layui-layer'+bindQQLayerIndex).css({'top': '8%', 'left': '50%', 'transform': 'translateX(-50%)'});
                $('.layui-layer-content').css('height', '318px');
                // 如果有其他的遮罩层，隐藏掉当前弹窗的遮罩层
                if ($('.pop-mask')[0]) {
                    $('#layui-layer-shade1').hide();
                }
                //localStorage.removeItem('needBindQQ');
                $(window).resize(function () {
                    $('#layui-layer'+bindQQLayerIndex).css({'top': '8%', 'left': '50%', 'transform': 'translateX(-50%)'});
                })
            }
            jQuery('a.menu-sub-text,a.tab-text,a.header-logo-link,.quick-dropdown-menu a,.icon-message').attr('href','javascript:void(0)').on('click',function(){
                showBindQQLayer();
                return false;
            });
        }
    })(jQuery);

});
</script>

    
    <script language="javascript" src="//pingjs.qq.com/tcss.ping.https.js"></script>

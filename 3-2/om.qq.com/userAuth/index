    <script type="text/javascript">
    var siteBaseUrl='https://om.qq.com/';
    (function () {
        try {
			 
            var e = navigator.userAgent, t = e.match(/iPhone|iPod/i);
            if(t || e.indexOf("Android") > -1 || e.indexOf("IEMobile") > -1){
				if(location.href.indexOf('/wenda/detail?wendaid=')>-1){
					var wendaid='';
					var reg = new RegExp("(^|&)" + 'wendaid' + "=([^&]*)(&|$)");
                    var r = window.location.search.substr(1).match(reg);
                    if (r != null) wendaid = unescape(r[2]);
					location.href = 'http://m.om.qq.com/mobile/wenda?wendaid='+wendaid;
					
				}else{
					location.href = 'http://m.om.qq.com/mobile/article';
				}
            }else{//pc端
                if(location.host.indexOf('m.om.qq.com')!=-1){
                    location.href = siteBaseUrl;
                }
            }
        }catch (n) {
        }
    })();
</script>

    
    
    

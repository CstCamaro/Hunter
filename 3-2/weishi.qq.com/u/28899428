    HYB.browserInfo.compareVersion = function(target, cmd) {
        if (!HYB.browserInfo.quaVersion) {return false;}
        var _compare = function(tokens1, tokens2, p) {
            if (!tokens1[p] && !tokens2[p]) {return 0;}
            return ((tokens1[p]||0) - (tokens2[p]||0)) || _compare(tokens1, tokens2, p + 1);
        };
        var r = _compare(HYB.browserInfo.quaVersion.split('.'), (target+'').split('.'), 0);
        r = r < 0 ? -1 : r > 0 ? 1 : 0;

        switch (cmd) {
            case 'eq' :
                return r === 0;
            case 'neq' :
                return r !== 0;
            case 'lt' :
                return r < 0;
            case 'nlt' :
                return r >= 0;
            case 'gt' :
                return r > 0;
            case 'ngt' :
                return r <= 0;
            default:
                return r;
        }
    };

    <script>
        (function(){
            
            var publicHeader = {
                    trim : function(str){
                        return str.replace(/(^\s*)|(\s*$)/g,'');
                    },
                    checkDomain : function(){
                        var domain = document.domain;
                        if(domain.indexOf("qq.com") < 0){
                            throw new Error('The domain is not "qq.com"!');
                        }else{
                            document.domain = "qq.com";
                        }
                    },
                    getCookie : function (name){
                        var cookie  = document.cookie;
                        var cookieArr = cookie.split(';');
                        for(var i=0;i<cookieArr.length;i++){
                            var cookieOne = cookieArr[i].split('=');
                            if(this.trim(cookieOne[0]) === this.trim(name)){
                                return cookieOne[1];
                            }
                        }
                    },
                    removeCookie : function(obj){
                        var expires = new Date();
                        expires.setTime(expires.getTime()-1);
                        if(!!this.getCookie(obj.name)){
                            document.cookie = obj.name+'=; expires='+expires+';path='+obj.path+';domain='+obj.domain;
                        }
                    }
            }
            var logout = document.getElementById('logout');
            if(!!logout){
                logout.onclick = function(){
                    publicHeader.checkDomain();
                    publicHeader.removeCookie({domain:'.qq.com',name:'uin',path:'/'});    
                    publicHeader.removeCookie({domain:'.qq.com',name:'skey',path:'/'});       
                    window.location.reload();
                }    
            }
            
        })(window)
        
    </script>
                        

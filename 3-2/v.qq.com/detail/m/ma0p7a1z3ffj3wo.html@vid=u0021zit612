	<!--[if lte IE 8]><script type="text/javascript">
		var resizeTimer = 0;
		function resizeInit() {
			var that = this
			
			if ($('html').hasClass('lte_ie8')) {
				that.ie8Width()
				
				$(window).resize(function () {
					clearTimeout(resizeTimer)
					resizeTimer = setTimeout(that.ie8Width, 100)
				})
			}
		};
		function ie8Width() {
			var MS = 'screen_ms';
			var XS = 'screen_xs';
			var LS = 'screen_ls';
			var XLS = 'screen_xls';
			var XXS = 'screen_xxs';
			
			var s = "CSS1Compat" == document.compatMode ? document.documentElement.clientWidth : document.body.clientWidth
			if (s <= 1280) {
				$('html').removeClass([MS, LS, XLS, XXS].join(' ')).addClass(XS)
			} else if (s <= 1549) {
				$('html').removeClass([XS, LS, XLS, XXS].join(' ')).addClass(MS)
			} else if (s <= 1789) {
				$('html').removeClass([MS, XS, XLS, XXS].join(' ')).addClass(LS)
			} else {
				$('html').removeClass([MS, XS, LS, XXS].join(' ')).addClass(XLS)
			}
		};
		resizeInit();
	</script><![endif]-->
